{
  "name": "atlasci-action",
  "version": "0.1.1",
  "description": "atlasci github action",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run codegen && npm run build && npm run format && npm run lint && npm test && npm run package",
    "shim": "ncc build migrate/apply/index.js -o migrate/apply/dist && ncc build migrate/push/index.js -o migrate/push/dist && ncc build migrate/lint/index.js -o migrate/lint/dist&& ncc build migrate/down/index.js -o migrate/down/dist",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ariga/ariga.git"
  },
  "keywords": [
    "atlas",
    "atlasci",
    "github",
    "action"
  ],
  "author": "ariga",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ariga/ariga/issues"
  },
  "homepage": "https://github.com/ariga/ariga#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.12",
    "@graphql-codegen/client-preset": "1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vercel/ncc": "^0.33.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "nock": "^13.2.9",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.3",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^1.7.2",
    "@octokit/rest": "^19.0.4",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "semver": "^7.5.4",
    "simple-git": "^3.11.0"
  }
}
